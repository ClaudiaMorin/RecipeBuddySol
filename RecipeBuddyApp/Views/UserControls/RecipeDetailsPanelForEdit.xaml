            </Grid.RowDefinitions>
            <StackPanel Grid.Column= "1" Grid.Row="0"  Orientation="Horizontal">
                <TextBlock Text="Author: "/>
                <TextBox x:Name="Author" TextAlignment="Left" Grid.Column= "1" Grid.Row="0" FontSize="14" TextWrapping="Wrap" Text="{Binding Path=Author, Mode=TwoWay}"/>
            </StackPanel>

            <StackPanel Orientation="Horizontal" Grid.Column="0" Grid.Row="0" >
                <TextBlock Text="  Category: " TextAlignment="Left" FontSize="14"/>
                <ComboBox x:Name="TypesInComboBox" SelectedIndex="{Binding CurrentType, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" >
                    <!--<i:Interaction.Triggers>
                        <i:EventTrigger EventName="SelectionChanged">
                            <i:InvokeCommandAction x:Name="ICommandSelectTypeChanged" Command="{Binding CmdSelectedTypeChanged}"  PassEventArgsToCommand="True" />
                        </i:EventTrigger>
                    </i:Interaction.Triggers>-->
                </ComboBox>
            </StackPanel>

            <TextBox x:Name="TotalTime" TextAlignment="Right" Grid.Column= "1" Grid.Row="0" FontSize="12" TextWrapping="Wrap" Text="{Binding Path=TotalTime, Mode=TwoWay}"/>
            <TextBlock x:Name="WebsiteHyperlink" TextAlignment="Right" Grid.Column= "1" Grid.Row="1" FontSize="12" TextWrapping="Wrap">
